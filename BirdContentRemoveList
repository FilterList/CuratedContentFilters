!Removes all Posts by specific users from appearing (Even when or if block is removed)
x.com##button[data-testid*='UserCell'] div div a span:has-text(/^@elonmusk$/i):upward(button[data-testid*='UserCell'])
x.com##div[data-testid^='cellInnerDiv']:not(:has-text(/Quote/i)) div[data-testid*='User-Name'] div:has-text(/^@elonmusk$/i):upward(div[data-testid^='cellInnerDiv'])
x.com##div[data-testid^='cellInnerDiv']:has-text(/Quote/i) div[data-testid*='User-Name'] div:has-text(/^@elonmusk$/i):upward(div[role^='link'])
x.com##span[class^='css']:has-text(/^@elonmusk$/i):upward(div[data-testid^='cellInnerDiv'])
x.com##span[class^='css']:has-text(/^@elonmusk$/i):upward(button[data-testid^='TypeaheadUser'])
x.com##button:has-text(/^go to @elonmusk$/i):upward([data-testid^='typeaheadResult'])
x.com##aside span[class^='css']:has-text(/^@elonmusk$/i):upward(li > div)
x.com##[data-testid*="primaryColumn"] :has-text(/^elon musk$/i):upward([role*="heading"]):upward(div)

x.com##button[data-testid*='UserCell'] div div a span:has-text(/^@Grummz$/i):upward(button[data-testid*='UserCell'])
x.com##div[data-testid^='cellInnerDiv']:not(:has-text(/Quote/i)) div[data-testid*='User-Name'] div:has-text(/^@Grummz$/i):upward(div[data-testid^='cellInnerDiv'])
x.com##div[data-testid^='cellInnerDiv']:has-text(/Quote/i) div[data-testid*='User-Name'] div:has-text(/^@Grummz$/i):upward(div[role^='link'])
x.com##span[class^='css']:has-text(/^@grummz$/i):upward(div[data-testid^='cellInnerDiv'])
x.com##span[class^='css']:has-text(/^@grummz$/i):upward(button[data-testid^='TypeaheadUser'])
x.com##button:has-text(/^go to @grummz$/i):upward([data-testid^='typeaheadResult'])
x.com##aside span[class^='css']:has-text(/^@grummz$/i):upward(li > div)
x.com##[data-testid*="primaryColumn"] :has-text(/^Grummz$/i):upward([role*="heading"]):upward(div)
